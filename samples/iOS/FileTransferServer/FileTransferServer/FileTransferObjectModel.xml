<xml>
    <node name="org/alljoyn/bus/samples/fileTransfer">
        <annotation name="org.alljoyn.lang.objc" value="FileTransferObject"/>
        <interface name="org.alljoyn.bus.samples.fileTransfer">
            <annotation name="org.alljoyn.lang.objc" value="FileTransferDelegate"/>
            <signal name="FileTransfer">
                <arg name="name" type="s">
                    <annotation name="org.alljoyn.lang.objc" value="TransferFileNamed:"/>
                </arg>
                <arg name="curr" type="u">
                    <annotation name="org.alljoyn.lang.objc" value="currentIndex:"/>
                </arg>
                <arg name="data" type="ay">
                    <annotation name="org.alljoyn.lang.objc" value="fileData:"/>
                </arg>
            </signal>
        </interface>
    </node>
</xml>
