<xml>
    <node name="org/alljoyn/MP3MediaStream">
        <annotation name="org.alljoyn.lang.objc" value="JJMP3MediaStream"/>        
        <interface name="org.alljoyn.MediaStream">
            <annotation name="org.alljoyn.lang.objc" value="JJMediaStreamDelegate"/>            
            <method name="Close">
                <annotation name="org.alljoyn.lang.objc" value="close"/>                
            </method>
            <method name="Open">
                <arg name="sessionName" type="s" direction="out">
                    <annotation name="org.alljoyn.lang.objc" value="openMediaInSession:"/>
                </arg>
                <arg name="sessionPort" type="q" direction="out">
                    <annotation name="org.alljoyn.lang.objc" value="onPort:"/>
                </arg>
                <arg name="success" type="b" direction="out">
                    <annotation name="org.alljoyn.lang.objc" value="result:"/>                    
                </arg>
            </method>
            <method name="Pause">
                <annotation name="org.alljoyn.lang.objc" value="pause"/>                
                <arg name="success" type="b" direction="out">
                </arg>
            </method>
            <method name="Play">
                <annotation name="org.alljoyn.lang.objc" value="play"/>                
                <arg name="success" type="b" direction="out">
                </arg>
            </method>
            <method name="SeekAbsolute">
                <arg name="position" type="u" direction="in">
                    <annotation name="org.alljoyn.lang.objc" value="seekToAbsolutePosition:"/>
                </arg>
                <arg name="units" type="y" direction="in">
                    <annotation name="org.alljoyn.lang.objc" value="positionUnits:"/>                    
                </arg>
                <arg name="success" type="b" direction="out">
                </arg>
            </method>
            <method name="SeekRelative">
                <arg name="offset" type="i" direction="in">
                    <annotation name="org.alljoyn.lang.objc" value="seekToRelativePosition:"/>                    
                </arg>
                <arg name="units" type="y" direction="in">
                    <annotation name="org.alljoyn.lang.objc" value="positionUnits:"/>                    
                </arg>
                <arg name="success" type="b" direction="out">
                </arg>
            </method>
            <property name="MimeType" type="s" access="read"/>
            <property name="Size" type="t" access="read"/>
            <property name="Seekable" type="y" access="read"/>
            <property name="Pausable" type="b" access="read"/>
        </interface>
        <interface name="org.alljoyn.MediaStream.Audio">
            <annotation name="org.alljoyn.lang.objc" value="JJAudioStreamDelegate"/>                     
            <property name="SampleFrequency" type="u" access="read"/>
            <property name="SamplesPerFrame" type="u" access="read"/>
            <property name="BitRate" type="u" access="read"/>
        </interface>
    </node>
</xml>